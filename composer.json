{
    "name": "rasta/ras-user-fetcher",
    "description": "Fetches Users",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Rasta\\RasUserFetcher\\": "."
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rasta\\RasUserFetcher\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "codeception/codeception": "*",
        "rasta/sniffs": "1.*",
        "phpunit/phpunit": "*"
    },
    "repositories": [
		{
            "type": "vcs",
            "url": "https://github.com/RapidAgileSoftware/ras-user-fetcher"
        }
	],
    "license": "GPL",
    "authors": [
        {
            "name": "Jens Krause",
            "email": "jens@rasta.online"
        }
    ],
    "scripts": {
        "test": "codecept run",
        "test:coverage": "codecept run unit --coverage --coverage-xml --coverage-html",
        "lint": "phpcbf --standard=phpcs.xml",
        "lint:fix": "phpcbf --standard=phpcs.xml"
    }
}